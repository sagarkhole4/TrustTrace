{
  "name": "verifier-service",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "dbSchema": "npx prisma db push --schema=./src/prisma/schema.prisma",
    "prisma:generate": "npx prisma generate --schema=./src/prisma/schema.prisma",
    "start": "nest start",
    "start:dev": "env-cmd -f .env nest start --watch",
    "start:pm2": "NATS_URL=nats://localhost:4222 pm2 start dist/src/main.js --name verifier-service --update-env",
    "start:debug": "env-cmd -f .env nest start --debug --watch",
    "start:prod": "npx prisma migrate deploy --schema=./prisma/schema.prisma & node dist/src/main.js",
    "start:docker": "NATS_URL=nats://localhost:4222 yarn start",
    "docker:build": "docker build -t verifier-service .",
    "docker:run": "docker run --name verifier-service -dp 3004:3004 verifier-service",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "prepare": "husky install",
    "pre-commit": "npx lint-staged"
  },
  "dependencies": {
    "@elastic/ecs-winston-format": "^1.3.1",
    "@nestjs/axios": "^3.0.0",
    "@nestjs/common": "^9.0.0",
    "@nestjs/config": "^2.2.0",
    "@nestjs/core": "^9.0.0",
    "@nestjs/jwt": "^10.0.3",
    "@nestjs/microservices": "^9.2.0",
    "@nestjs/passport": "^9.0.3",
    "@nestjs/platform-express": "^9.0.0",
    "@nestjs/swagger": "^6.1.3",
    "@nestjs/terminus": "^10.0.1",
    "@prisma/client": "^4.15.0",
    "amazon-cognito-identity-js": "^6.2.0",
    "axios": "^1.6.5",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "helmet": "^7.1.0",
    "joi": "^17.6.4",
    "jwks-rsa": "^3.0.1",
    "nats": "^2.9.0",
    "nestjs-cls": "^3.5.0",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.1",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.5.7",
    "uuid4": "^2.0.3",
    "winston": "3.4",
    "winston-elasticsearch": "0.16.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^9.0.0",
    "@nestjs/schematics": "^9.0.0",
    "@nestjs/testing": "^9.0.0",
    "@types/express": "^4.17.13",
    "@types/jest": "28.1.8",
    "@types/node": "^16.0.0",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "env-cmd": "^10.1.0",
    "eslint": "^8.38.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": "^8.0.0",
    "jest": "28.1.3",
    "lint-staged": "^13.2.1",
    "prettier": "^2.8.7",
    "prisma": "^4.15.0",
    "source-map-support": "^0.5.20",
    "supertest": "^6.1.3",
    "ts-jest": "28.0.8",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.0.0",
    "tsconfig-paths": "4.1.0",
    "typescript": "^4.7.4"
  },
  "lint-staged": {
    "src/**/*.{ts}": "prettier --write",
    "src/**/*.ts": "eslint --fix"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
